function set_loading() {
	$('#loading').show();
}

function clear_loading() {
	$('#loading').hide();
}

function set_loading_with_id(id) {
	$('#loading-'+id).show();
}

function clear_loading_with_id(id) {
	$('#loading-'+id).hide();
}

function set_search_autocomplete(selector) {
	$(selector).autocomplete({
		source: "/names/ajax_search_autocomplete.php",
		minLength: 1, // limited to 2 characters below, unless CJK character
		select: function (event, ui) {
			if (ui.item) {
				if (ui.item.value.charAt(0) != '{') {
					this.value = ui.item.label.replace(/<(?:.|\n)*?>/gm, '');
					window.location.href = "/name/" + ui.item.value;
				} else {
					var p = ui.item.value.indexOf('}');
					if (p > 0) {
						var cmd = ui.item.value.substring(1, p);
						var rest = ui.item.value.substring(p+2);
						var p = cmd.indexOf('-');
						if (p > 0) {
							var subcmd = cmd.substring(p+1);
							cmd = cmd.substring(0, p);
						}
						switch (cmd) {
  						case 'find':
							window.location.href = "/names/search.php?terms=" + rest;
     						break;
  						case 'pending':
							window.location.href = "/name/" + rest + "/submitted";
    						break;
  						case 'top':
							var p = rest.indexOf(' ');
							if (p > 0) {
								window.location.href = "/top/name/" + rest.replace(/ +/g, ',');
							} else {
								window.location.href = "/name/" + rest + "/top";
							}
     						break;
  						case 'namesake':
							var p = rest.indexOf(' ');
							if (p > 0) {
								window.location.href = "/namesakes/name/" + rest.replace(/ +/g, ',');
							} else {
								window.location.href = "/name/" + rest + "/namesakes";
							}
     						break;
  						case 'related':
							window.location.href = "/names/related/" + rest.replace(/ +/g, ',');
     						break;
  						case 'nameday':
							var p = rest.indexOf(' ');
							if (p > 0) {
								window.location.href = "/namedays/name/" + rest.replace(/ +/g, ',');
							} else {
								window.location.href = "/name/" + rest + "/namedays";
							}
    						break;
  						case 'meaning':
							window.location.href = "/names/meaning/" + rest.replace(/ +/g, ',');
     						break;
  						case 'surname':
							window.location.href = "//surnames.behindthename.com/name/" + rest;
     						break;
  						case 'place':
							window.location.href = "//places.behindthename.com/name/" + rest;
     						break;
  						case 'given':
							window.location.href = "//www.behindthename.com/name/" + rest;
     						break;
  						case 'root':
							window.location.href = "/element/" + rest;
     						break;
  						case 'roots':
							window.location.href = "/elements/pattern/" + rest.replace(/ +/g, ',');
     						break;
  						case 'user':
							window.location.href = "/members/search.php?terms=" + encodeURIComponent(rest);
     						break;
  						case 'glossary':
							window.location.href = "/glossary/search.php?terms=" + encodeURIComponent(rest);
     						break;
  						case 'browse':
							window.location.href = rest;
     						break;
						}
					}
				}
			}
			return false;
		},
    	/*search: function(event, ui) { // only allow searching 1 character length if Han character
			var value = $(selector).val();
			if (value.length==1) {
				if (value.match(/[\u4e00-\u9faf]/)) { // is CJK chracter
					;
				} else {
					event.preventDefault();
				}
			}
      	},*/
		focus: function (event, ui) {
			return false;
		},
		open: function () {
        	$(this).data("ui-autocomplete").menu.element.addClass("search-suggestions");
    	},
    	create: function() {
    		$(this).data('ui-autocomplete')._renderItem = function( ul, item )  {
				if (item.value) {
					return $( "<li>" )
						.append( "<a>" + item.label + "</a>" )
						.appendTo( ul );
				} else {
					return $( "<li>" )
						.append( item.label )
						.appendTo( ul );
				}
    		}
    	}
	});
}

function toggle_div(id) {	
	$('#'+id).toggle();
}

function click_outside(selector, callback) {
	// do something (ie close the popup) when clicked outside
	event.stopPropagation();
	$(document).off('click'); // remove any previous
	$(document).click(function(event) {
		if (!$(event.target).closest(selector).length) {
			if (callback) {
				callback();
			}
		}
	});
}
function click_outside_remove() {
 	$(document).off('click'); // remove click listener
}

function preload_image(path) {
    $('<img/>')[0].src = '/images/'+path;
}

function show_namedesc(name, span_id) {	
	$.get('/names/ajax_name_desc.php?name='+name, function(data) {
		$('#'+span_id).html(data);
	});
	return false;
}

function show_prondetail(pron, num, ipa, ipapref) {
	if ($('#pron-detail-'+num).is(':visible')) {
		hide_prondetail();
	} else {
		if (ipa) ipa = 1;
		else ipa = 0;
		if (ipapref) ipapref = 1;
		else ipapref = 0;
		if (arguments.length>=5) {
			var usages = arguments[4];
		} else {
			var usages = '';
		}
		$.get('/names/ajax_pron_detail.php?pron='+pron+'&ipa='+ipa+'&ipapref='+ipapref+'&u='+usages, function(data) {
			$('.pron-detail').hide();
			$('#pron-detail-'+num).html(data);
			$('#pron-detail-'+num).show();
		});

		click_outside('.popup-pointy', hide_prondetail);
	}
	
	return false;
}

function hide_prondetail() {	
	$('.popup-wrapper').hide();
	click_outside_remove();
}

function toggle_pron(name, ipa, div_id) {
	if (ipa) ipa = 1;
	else ipa = 0;
	$.get('/names/ajax_toggle_pron.php?name='+name+'&ipa='+ipa, function(data) {
		$('#'+div_id).html(data);
	});
	return false;
}

function set_setting_from_checkbox(key, cb, token) {
	var checked = $(cb).prop('checked');
	var value = '0';
	if (checked) value = '1';
	return set_setting(key, value, token);
}

function set_setting(key, value, token) {
	$.get('/members/ajax_setting.php?key='+key+'&value='+value+'&token='+token, function(data) {
		data = data.trim();
		if (data=='ok') {
			set_info_message('Your preference has been saved.');
		} else {
			set_info_message('');
			if (data!='') {
				alert(data);
			}
		}
	});
	return false;
}

function set_theme(theme, version, token) {
	if (theme=='dark') {
		var css = "/style.php?v="+version+"&theme=dark";
		var img_src = "/images/btn-darkmode.png";
	} else {
		var css = "/style.php?v="+version;
		var img_src = "/images/btn.png";
	}
	$('head').append('<link rel="stylesheet" type="text/css" href="'+css+'">');
	$('#logo img').attr("src", img_src);
	set_setting('_theme', theme, token);
	if (theme=='dark') {
		$('.swap-theme-dark').hide();
		$('.swap-theme-light').show();
		$('.swap-theme-light').css('display', 'inline-block');
	} else {
		$('.swap-theme-light').hide();
		$('.swap-theme-dark').show();
		$('.swap-theme-dark').css('display', 'inline-block');
	}
	return false;
}

function add2pnl_name(name, member_id, list_id, remove, quick, token) {
	if (member_id==0) {
		window.location.href = "/members/login";
		return;
	}
	if (quick && ($('#pnl-detail-'+name).is(":visible"))) {
		hide_pnldetails();
	} else {
		var page = 'ajax_doname.php';
		$.get('/pnl/'+page+'?name='+name+'&list='+list_id+'&remove='+(remove ? 'yes' : '')+'&quick='+(quick ? 'yes' : '')+'&token='+token, function(data) {
			var res = $.parseJSON(data);
			set_info_message(res.msg);
			if (res.ok=='1') {
				$('.pnlicon').html(res.icon);
				flashicon('.pnlicon img');
				$('#pnl-add-'+name).html(res.btn);
				$('#pnl-pop-content-'+name).html(res.list);
			}
		});
	}
	return false;
}

function add2pnl_details(name, member_id, token) {
	if (member_id==0) {
		window.location.href = "/members/login";
		return;
	}
	if ($('#pnl-detail-'+name).is(":visible")) {
		hide_pnldetails();
	} else {
		preload_image('name/star-black.png');
		preload_image('name/star-yellow.png');
		var page = 'ajax_details.php';
		$.get('/pnl/'+page+'?name='+name, function(data) {
			$('.pnl-detail').hide();
			$('#pnl-detail-'+name).html(data);
			$('#pnl-detail-'+name).show();
		});

		click_outside('.popup-pointy', hide_pnldetails);
	}
}

function hide_pnldetails() {	
	$('.popup-wrapper').hide();
	click_outside_remove();
}

function set_info_message(msg) {
	if (msg=='') {
		$('.info_message').html('');
	} else {
		if ($('.info_message').html()=='') {
			$('.info_message').hide();
			$('.info_message').html(msg);
			$('.info_message').slideDown(200);
		} else {
			$('.info_message').html(msg);
		}
	}
}

function expand_name_links(name, div_id) {
	if ($('#'+div_id).html()=='') {
		$.get('/names/ajax_expand_links.php?name='+name, function(data) {
			$('#'+div_id).html(data);
			$('#'+div_id).slideDown(200);
		});
	} else {
		$('#'+div_id).slideToggle(200);
	}
	return false;
}

function flashicon(selector) {
	$(selector).delay(100).fadeTo(100,0.5).delay(100).fadeTo(100,1).delay(100).fadeTo(100,0.5).delay(100).fadeTo(100,1).delay(100).fadeTo(100,0.5).delay(100).fadeTo(100,1);
}

function despoil(selector) {
	if ($(selector).hasClass('spoiler')) { // do any have this class?
		$(selector).removeClass('spoiler', 'fast');
	} else {
		$(selector).addClass('spoiler', 'fast');
	}
	return false;
}

function vote(type, dir, id, member_id, uvo, token) {
	if (member_id==0) {
		alert('You must be logged in to vote.');
		return;
	}
	$.get('/votes/ajax_vote.php?type='+type+'&dir='+dir+'&id='+id+'&mid='+member_id+'&uvo='+uvo+'&token='+token, function(data) {
		if (data.charAt(0)=='-') {
			alert(data.substr(1));
		} else {
			$('#vote-'+id).html(data);
		}
	});
}

function discuss_form(accentname, content_type, parent_id, target_div_id) {
	$.get('/discuss/ajax_form.php?name='+accentname+'&content_type='+content_type+'&parent_id='+parent_id, function(data) {
		$('#'+target_div_id).html(data);
	});
	return false;
}

function discuss_submit(form, target_div_id) {
	$.post('/discuss/ajax_doadd.php', $(form).serialize(), function(data) {
		$('#'+target_div_id).html(data);
	});
	return false;
}

function discuss_cancel(accentname, content_type, target_div_id) {
	$.get('/discuss/ajax_docancel.php?name='+accentname+'&content_type='+content_type, function(data) {
		$('#'+target_div_id).html(data);
	});
	return false;
}

function hist_load(div_id, name_id, version, pending) {
	set_loading_with_id(div_id);
	if (pending) {
		var url = '/submit/ajax_history_version.php';
	} else {
		var url = '/names/ajax_history_version.php';
	}
	$.get(url+'?id='+name_id+'&version='+version, function(data) {
		$('#nv-content-'+div_id).html(data);
		clear_loading_with_id(div_id);
		$('.nv-pointer-'+div_id).hide();
		if (pending) {
			$('#nv-pointer-'+div_id+'-u'+version).show();
		} else {
			$('#nv-pointer-'+div_id+'-'+version).show();
		}
	});
	return false;
}


/* name browser */
function nb_toggle_refine(quick) {
	if (quick) {
		$('#form-refine-quick').hide();
		$('#form-refine').slideDown('fast');
	} else {
		$('#form-refine').slideUp('fast', function() {
			$('#form-refine-quick').show();
		});
	}
	return false;
}

function nb_submit_form(form) {	
	$('.nb-data').filter(':hidden').find(':input').attr('disabled', true);
	$('.nb-on').attr('disabled', true);
	return true;
}

function nb_toggle_option(id) {
	if ($('#nb-option-'+id).hasClass('nb-refine-on')) {
		$('#nb-option-'+id).removeClass('nb-refine-on');
		$('#nb-plusminus-'+id).text('+');
		$('#nb-option-'+id+' div.nb-data').slideUp('fast');
	} else {
		$('#nb-option-'+id).addClass('nb-refine-on');
		$('#nb-plusminus-'+id).text('-');
		$('#nb-option-'+id+' div.nb-data').slideDown('fast');
	}
	return false;
}

function nb_toggle_help(id) {
	if ($('#nb-help-'+id).is(':hidden')) {
		$('#nb-help-'+id).show();
		$('#nb-help-'+id+' > td > ul').slideDown('fast');
	} else {
		$('#nb-help-'+id+' > td > ul').slideUp('fast', function() { 
			$('#nb-help-'+id).hide();
		});
	}
	return false;
}

function nb_advanced(form_id, url, advanced) {	
	var params = $("#"+form_id).serializeArray();
	params.push({name: 'advanced', value: advanced});
	$.post(url, params, function(data) {
		$('#nb-refine-list').slideUp('fast', function() {
			$('#nb-refine-list').html(data);
			$('#nb-refine-list').slideDown('fast');
		});
		if (advanced) {
			$('#nb-advanced-simple').show();
			$('#nb-advanced-complex').hide();
		} else {
			$('#nb-advanced-complex').show();
			$('#nb-advanced-simple').hide();
		}
	});
	return false;
}

function nb_toggle_select_number(op_cb, select_id, op_limit, multi_spots) {
	if (op_cb.selectedIndex<op_limit) {
		$("#"+select_id).removeAttr("multiple");
		$("#"+select_id).attr("size", "1");
	} else {
		$("#"+select_id).attr("multiple", "multiple");
		$("#"+select_id).attr("size", multi_spots);
	}
}
function nb_filter_redir(select) {
	window.location = select.options[select.selectedIndex].value;
}
function nb_filtertext_redir(input, url, placeholder) {
	window.location = url.replace(encodeURIComponent(placeholder), encodeURIComponent(input.value));
}
function nb_filtertext_redir_key(event, input, url, placeholder) {
    if (event.key === "Enter") {
    	event.preventDefault();
        nb_filtertext_redir(input, url, placeholder);
    }
}
function nb_change_value(partial_id) {
	if ($("#nb-input-"+partial_id).val()=='') {
		$("#nb-row-"+partial_id).removeClass('nb-row-on');
	} else {
		$("#nb-row-"+partial_id).addClass('nb-row-on');
	}
}
function nb_change_value_switch(switch_name, partial_id) {
	if ($('input[name="'+switch_name+'"]:checked').val()=='') {
		$("#nb-row-"+partial_id).removeClass('nb-row-on');
	} else {
		$("#nb-row-"+partial_id).addClass('nb-row-on');
	}
}
function nb_clear_value(partial_id, input_type) {
	if (input_type=='select') {
		$("#nb-input-"+partial_id+" option:selected").prop("selected", false);
	} else {
		$("#nb-input-"+partial_id).val("");
	}
	nb_change_value(partial_id);
}
function nb_neg_checkbox(partial_id) {
	if ($("#nb-input-"+partial_id).is(':checked')) { // checked = disable hidden field
		$("#nb-neginput-"+partial_id).prop('disabled', true);
	} else {
		$("#nb-neginput-"+partial_id).prop('disabled', false);
	}
}
/* end name browser */

function show_mobilemenu() {
	$("#mobilemenu").show();
	$("#mobilemenu").css("width", "100%");
	$("#mobileopts-icon-open").hide();
	$("#mobileopts-icon-close").show();
	$("body").css('overflow', 'hidden');
	$("#menubar-wrapper").css('position', 'fixed');
}

function hide_mobilemenu() {
	$("#mobilemenu").css("width", "0px");
	$("#mobilemenu").hide();
	$("#mobileopts-icon-close").hide();
	$("#mobileopts-icon-open").show();
	$("body").css('overflow', 'auto');
	$("#menubar-wrapper").css('position', 'absolute');
}

function transform_title(new_title, new_center, new_nav) {
	$('#pageheading-text').text(new_title);
	$('#pageheading-nav').html(new_nav);
	if (new_center) {
		$('.pageheading').addClass('pageheading-center');
	} else {
		$('.pageheading').removeClass('pageheading-center');
	}
}

$(document).ready(function() {

	console.log("BTN menus go");
	
	// top menus
	$("#btnmenu li").click(function() { // clicked...
		$(this).find("ul.btnsubmenu").show();
	}).hover(function() { // on hover...
		$(this).find("ul.btnsubmenu").show();
		$(this).addClass("btnsubhover");
	}, function() { // on hover out...
		$(this).parent().find("ul.btnsubmenu").hide();
		$(this).removeClass("btnsubhover");
	});

	// member menu
	$("#memberopts").click(function() { // clicked...
		$("#membermenu").show();
	}).hover(function() { // on hover...
		$("#membermenu").show();
		$(this).addClass("btnsubhover");
	}, function() { // on hover out...
		$("#membermenu").hide();
		$(this).removeClass("btnsubhover");
	});

	// mobile menu
	$("#mobileopts").click(function() { // clicked...
		if ($("#mobilemenu").is(":visible") == true) {
			hide_mobilemenu();
		} else {
			show_mobilemenu();
		}
	});

	// any hover buttons (pnl, pm, bb)
	init_hoverbutton();

	//console.log("BTN menus done");
});


function init_hoverbutton() {
	$(".hoverbutton").click(function() {
		$(this).children("div").finish().toggle();
	}).hover(function() {
		$(this).children("div").finish().show();
	}, function() {
		$(this).children("div").finish().hide();
	});
}

function copy2clipboard(text) {
	navigator.clipboard.writeText(text).catch(() => {
		alert("your browser does not support this");
    });
    return false;
}

if (!String.prototype.trim) {
	String.prototype.trim = function() {
		return this.replace(/^\s+|\s+$/g, ''); 
	};
}

if (!Array.prototype.indexOf) {
	Array.prototype.indexOf = function (obj) {
		for (var i=0, j=this.length; i<j; i++) {
			if (this[i]===obj)
				return i;
		}
		return -1;
	};
}

